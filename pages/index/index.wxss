
.index_swiper{
  margin: 10rpx auto;
  width: 750rpx;
  /* height: calc(750rpx * 280 / 520); */
  height: 300rpx;
  border-radius: 20rpx;
   text-align: center;
   justify-content: center;
   overflow: hidden;
}
.index_swiper swiper-item image{
  width: 100%;
  height: 100%;
}
.clear_fix::after{
  content:"";
  display: block;
  clear: both;
}
.index_cate{
  margin: 20rpx auto;
  text-align: center;
  padding: 5rpx;
  background-color: #f3f3f3;
  justify-content: center;
  align-items: center;
  border-radius: 20rpx;
}
.index_cate navigator{
  display: inline-block;
  padding: 15rpx;
  padding-top: 0;

}
.index_cate navigator view{
  
  width: 150rpx;
  height: 150rpx;
  text-align: center;
  padding-bottom: 6rpx;
  font-weight: bold;

}
.index_cate navigator image{
  width: 100%;

}
